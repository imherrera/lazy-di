{
  "name": "composed-di",
  "private": false,
  "version": "0.3.1",
  "author": "Juan Herrera juanhr454@gmail.com",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "keywords": [
    "dependency injection",
    "di",
    "lazy dependency injection"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imherrera/composed-di.git"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.10",
    "@types/node": "24.0.10",
    "prettier": "^3.3.1",
    "typescript": "^5.2.2",
    "vitest": "^4.0.16"
  }
}
